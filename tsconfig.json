{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES2022",
    "moduleResolution": "node",
    "allowJs": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}