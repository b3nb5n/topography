{
	"name": "topography-cms",
	"version": "0.1.0",
	"description": "An open-source content managment system",
	"repository": "https://github.com/baldwin-dev-co/topography-cms.git",
	"author": "Ben Baldwin <benbaldwin000@gmail.com>",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"client",
		"prisma",
		"server/*",
		"common/*"
	],
	"scripts": {
		"test": "lerna run test --since",
		"build": "lerna run build",
		"dev": "lerna run dev --parallel",
		"version": "lerna version --no-push",
		"db:start": "docker run --name topography_db -p 5432:5432 -e POSTGRES_PASSWORD=password -d postgres"
	},
	"devDependencies": {
		"lerna": "^4.0.0"
	},
	"packageManager": "yarn@3.2.0",
	"dependencies": {
		"@prisma/client": "3.12.0"
	}
}
