{
	"name": "@topography/utils",
	"packageManager": "yarn@3.2.0",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"tsup": {
		"entry": [
			"src/**/*.ts"
		],
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"dependencies": {
		"zod": "^3.14.4"
	},
	"devDependencies": {
		"tsup": "^5.12.6"
	}
}
